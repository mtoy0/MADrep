        <columns>
          <TableColumn fx:id="classNameColumn" prefWidth="146.0" text="ClassName" />
          <TableColumn fx:id="classIdColumn" prefWidth="104.0" text="ClassID" />
        </columns>
      </TableView>
      <TableView fx:id="studentTableView" layoutX="334.0" layoutY="50.0" prefHeight="200.0" prefWidth="200.0">
