          <TableColumn fx:id="classListColumn" prefWidth="199.0" text="ClassList" />
        </columns>
      </TableView>
      <Label layoutX="49.0" layoutY="264.0" text="Search Classes" />
      <TextField fx:id="classField" layoutX="49.0" layoutY="281.0" prefHeight="26.0" prefWidth="251.0" />
