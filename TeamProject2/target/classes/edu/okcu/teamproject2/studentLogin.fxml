<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.okcu.teamproject2.studentLoginController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Login">
         <font>
            <Font size="50.0" />
         </font></Label>
      <PasswordField fx:id="passwordPasswordField" layoutX="15.0" layoutY="124.0" onAction="#onPasswordPasswordFieldEntered" promptText="Password" />
      <TextField fx:id="emailTextField" layoutX="15.0" layoutY="88.0" onAction="#onEmailTextFieldEntered" promptText="Email" />
      <Button fx:id="cancelButton" layoutX="15.0" layoutY="239.0" mnemonicParsing="false" onAction="#onCancelButtonClicked" text="Cancel" />
      <Button fx:id="loginButton" layoutX="15.0" layoutY="206.0" mnemonicParsing="false" onAction="#onLoginButtonClicked" text="Login" />
   </children>
</AnchorPane>
